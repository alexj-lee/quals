	urldate = {2023-04-04},
	date = {2018-05-17},
	langid = {english},
	keywords = {circuit, hippocampus, memory, {RNA}-seq, subiculum},
	file = {ScienceDirect Full Text PDF:/home/ajl/Zotero/storage/78XIY8Q8/Cembrowski et al. - 2018 - Dissociable Structural and Functional Hippocampal .pdf:application/pdf;ScienceDirect Snapshot:/home/ajl/Zotero/storage/TTLVA3YP/S0092867418303118.html:text/html},
}

@article{cembrowski_spatial_2016,
	title = {Spatial Gene-Expression Gradients Underlie Prominent Heterogeneity of {CA}1 Pyramidal Neurons},
